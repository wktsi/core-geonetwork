<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <skos:ConceptScheme xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope">
    <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="en">Spatial scope</dc:title>
    <dc:description xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="en">Administrative level that the data set intends to cover.</dc:description>
    <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="es">Ámbito espacial</dc:title>
    <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="fr">Champ géographique</dc:title>
    <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="pt">Âmbito geográfico</dc:title>
    <dcterms:issued xmlns:dcterms="http://purl.org/dc/terms/">2019-10-14T10:04:24</dcterms:issued>
    <dcterms:modified xmlns:dcterms="http://purl.org/dc/terms/">2019-10-14T10:04:24</dcterms:modified>
  </skos:ConceptScheme>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope/national">
    <skos:prefLabel xml:lang="en">National</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The scope of the data set intends to cover the national territory.</skos:scopeNote>
    <skos:prefLabel xml:lang="es">Nacional</skos:prefLabel>
    <skos:scopeNote xml:lang="es">El ámbito de aplicación del conjunto de datos se propone cubrir el territorio nacional.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Nationales</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">Le champ d’application de la série de données vise à couvrir le territoire national.</skos:scopeNote>
    <skos:prefLabel xml:lang="pt">Nacional</skos:prefLabel>
    <skos:scopeNote xml:lang="pt">O âmbito do conjunto de dados pretende abranger o território nacional.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope" />
  </skos:Concept>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope/regional">
    <skos:prefLabel xml:lang="en">Regional</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The scope of the data set intends to cover the territory of one or several sub-national level administrative units.</skos:scopeNote>
    <skos:prefLabel xml:lang="es">Regionales</skos:prefLabel>
    <skos:scopeNote xml:lang="es">El ámbito de aplicación del conjunto de datos se propone cubrir el territorio de una o varias unidades administrativas de nivel subnacional.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Régional</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">Le champ d’application de la série de données vise à couvrir le territoire d’une ou de plusieurs unités administratives de niveau infranational.</skos:scopeNote>
    <skos:prefLabel xml:lang="pt">Regional</skos:prefLabel>
    <skos:scopeNote xml:lang="pt">O âmbito do conjunto de dados pretende abranger o território de uma ou mais unidades administrativas a nível subnacional.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope" />
  </skos:Concept>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope/local">
    <skos:prefLabel xml:lang="en">Local</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The scope of the data set intends to cover the territory of one or several local administrative units.</skos:scopeNote>
    <skos:prefLabel xml:lang="es">Local</skos:prefLabel>
    <skos:scopeNote xml:lang="es">El ámbito de aplicación del conjunto de datos se propone cubrir el territorio de una o varias unidades administrativas locales.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Local</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">La portée de l’ensemble de données vise à couvrir le territoire d’une ou de plusieurs unités administratives locales.</skos:scopeNote>
    <skos:prefLabel xml:lang="pt">Local</skos:prefLabel>
    <skos:scopeNote xml:lang="pt">O âmbito do conjunto de dados pretende abranger o território de uma ou várias unidades administrativas locais.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope" />
  </skos:Concept>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope/global">
    <skos:prefLabel xml:lang="en">Global</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The scope of the data set intends to cover the whole Earth.</skos:scopeNote>
    <skos:prefLabel xml:lang="es">Global</skos:prefLabel>
    <skos:scopeNote xml:lang="es">El alcance del conjunto de datos se propone cubrir todo el planeta.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Mondial</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">Le champ d’application de la série de données vise à couvrir l’ensemble de la Terre.</skos:scopeNote>
    <skos:prefLabel xml:lang="pt">Mundial</skos:prefLabel>
    <skos:scopeNote xml:lang="pt">O âmbito do conjunto de dados pretende abranger toda a Terra.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope" />
  </skos:Concept>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope/european">
    <skos:prefLabel xml:lang="en">European</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The scope of the data set intends to cover Europe.</skos:scopeNote>
    <skos:prefLabel xml:lang="es">Europea</skos:prefLabel>
    <skos:scopeNote xml:lang="es">El ámbito de aplicación del conjunto de datos se propone cubrir Europa.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Européen</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">Le champ d’application de la série de données vise à couvrir l’Europe.</skos:scopeNote>
    <skos:prefLabel xml:lang="pt">Europeia</skos:prefLabel>
    <skos:scopeNote xml:lang="pt">O âmbito do conjunto de dados pretende abranger a Europa.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/SpatialScope" />
  </skos:Concept>
</rdf:RDF>

